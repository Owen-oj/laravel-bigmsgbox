{
    "name": "owenoj/laravel-bigmsgbox",
    "description": "Laravel notification channel for bigmsgbox ",
    "keywords": [
        "owenoj",
        "laravel-bigmsgbox"
    ],
    "homepage": "https://github.com/owenoj/laravel-bigmsgbox",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Owen Jubilant ",
            "email": "owen.j@terktrendz.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Owenoj\\LaravelBigmsgbox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Owenoj\\LaravelBigmsgbox\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Owenoj\\LaravelBigmsgbox\\LaravelBigmsgboxServiceProvider"
            ],
            "aliases": {
                "LaravelBigmsgbox": "Owenoj\\LaravelBigmsgbox\\LaravelBigmsgboxFacade"
            }
        }
    }
}
